<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="fs.four.dropout.community.dao.CommunityWriteDAO">
    <resultMap id="communityResult" type="communityVO">
<!--        <result property="com_post_number" column="com_post_number"/>-->
<!--        <result property="usr_id" column="usr_id"/>-->
        <result property="com_title" column="com_title"/>
<!--        <result property="com_thumbnail_name" column="com_thumbnail_name"/>-->
        <result property="com_location" column="com_location"/>
        <result property="com_content" column="com_content"/>
    </resultMap>

    <insert id="insertCommunityWrite" parameterType="communityVO">
<!--        <selectKey keyProperty="com_post_number">-->

<!--            SELECT SEQ_POST.nextval From DUAL;-->

<!--        </selectKey>-->

        <![CDATA[
        insert into lm_community(com_title, com_location, com_content)
        values (#{com_title}, #{com_location}, #{com_content})
        ]]>
    </insert>

</mapper>